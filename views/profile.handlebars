{{#if message}}
    <p>{{message}}</p>
{{/if}}
<table>
    <tr>
        <th>Name</th>
        <th>Value</th>
    </tr>
    {{#each profile}}
        <tr>
            <td>{{@key}}</td>
            <td>{{this}}</td>
        </tr>
    {{/each}}
</table>
<table>
        <tr>
            <td>
                <table>
                    <tr>
                        <th>Name</th>
                        <th>Level</th>
                    </tr>
                    {{#each instruments}}
                        <tr>
                            <td>{{InstrumentKey}} - {{Instrument}}</td>
                            <td>{{LevelKey}} - {{Level}}</td>
                        </tr>
                    {{/each}}
                </table>
            </td>
        </tr>
</table>
<table>
        <tr>
            <td>
                <table>
                    <tr>
                        <th>Location</th>
                    </tr>
                    {{#each workSamples}}
                        <tr>
                            <td>{{SampleKey}} - {{SampleLocation}}</td>
                        </tr>
                    {{/each}}
                </table>
            </td>
        </tr>
</table>